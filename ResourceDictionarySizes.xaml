<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:Double x:Key="IconWidth">30</sys:Double>
    <sys:Double x:Key="IconHeight">30</sys:Double>
    <sys:Double x:Key="IconWidthSmall">20</sys:Double>
    <sys:Double x:Key="IconHeightSmall">20</sys:Double>
    <Thickness x:Key="IconPadding">3 3 3 4</Thickness>
    <Thickness x:Key="IconMargin">2 2 2 2</Thickness>
    <Thickness x:Key="ControlMargin">0 5 0 0</Thickness>
    <sys:Double x:Key="FontSizeMain">12</sys:Double>
    <sys:Double x:Key="FontSizeFlyout">12</sys:Double>
    <sys:Double x:Key="FontSizeCalib">12</sys:Double>
    <sys:Double x:Key="FontSizeChart">14</sys:Double>
    
</ResourceDictionary>